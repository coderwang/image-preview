{
  "name": "image-preview",
  "displayName": "Image Preview",
  "description": "Preview all images in the folder",
  "keywords": [
    "image",
    "preview",
    "folder"
  ],
  "version": "0.0.1",
  "publisher": "coderwsh",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/coderwang/image-preview"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.imagePreview",
        "title": "Image Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.imagePreview",
          "when": "explorerResourceIsFolder",
          "group": "z_commands"
        }
      ],
      "commandPalette": [
        {
          "command": "extension.imagePreview",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build & npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode production --watch"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^16.x",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.80.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "rc-slider": "^11.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.7.4"
  }
}
