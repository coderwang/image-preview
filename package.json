{
  "name": "image-preview",
  "displayName": "Image Preview",
  "description": "Preview all images in the folder",
  "keywords": [
    "image",
    "preview",
    "folder"
  ],
  "version": "0.0.1",
  "publisher": "coderwsh",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/coderwang/image-preview"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.imagePreview",
        "title": "Image Preview"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.imagePreview",
          "when": "explorerResourceIsFolder",
          "group": "z_commands"
        }
      ],
      "commandPalette": [
        {
          "command": "extension.imagePreview",
          "when": "false"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/vscode": "^1.96.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
